<BaseProject
  ProjectTemplateFile="@(ProjectConfigName)_project"
  IncludeDirectories=""
  LibraryDirectories=""
  LinkLibraries=""
  PreprocessorDefinitions=""
  PrecompiledHeader=""
  HLSLShaderMain="main"
  HLSLAdditionalFlags=""
  SXFXAdditionalArguments=""
  CUDACustomParameters=""

  <!-- default Visual Studio settings -->
  VSAdditionalOptions="/bigobj"
  VSDisableSpecificWarnings=""
  VSWarningLevel="@(VSWarningLevel_4)"
  VSUsePrecompiledHeader="@(VSPCH_Disabled)"
  VSCompileAs="@(VSCompileAs_Default)"
  VSDebugOptimization="@(VSOptimization_Disabled)"
  VSDebugBasicRuntimeChecks="@(VSBasicRTChecks_Both)"
  VSSubSystem="@(VSSubSystem_Windows)"
  VSMultiProcessorCompilation="true"
  VSMinimalRebuild="false"
  VSCompileAsWinRT="false"
/>

<PropertyListInputFormats>
  <PropertyList Name="IncludeDirectories" Separator=";" />
  <PropertyList Name="PreprocessorDefinitions" Separator=";" />
  <PropertyList Name="LinkLibraries" Separator=";" />
  <PropertyList Name="Files" Separator=" " />
</PropertyListInputFormats>

<VS2008
  SupportedPlatforms="win32 win64 x360"
  ProjectConfigName="vs2008"
  ProjectOutputDir="vs2008"
  DefaultCompilerName="vs2008"
  DefaultCompilerDefinition="PFC_COMPILER_MSVC2008"
  DXSDK_INCLUDE_DIR="$(DXSDK_DIR)Include"

  <!-- standard Visual Studio property values -->
  VSWarningLevel_Off="0"
  VSWarningLevel_1="1"
  VSWarningLevel_2="2"
  VSWarningLevel_3="3"
  VSWarningLevel_4="4"
  VSWarningLevel_All="4"
  VSOptimization_Disabled="0"
  VSOptimization_MinSize="1"
  VSOptimization_MaxSpeed="2"
  VSOptimization_Full="3"
  VSPCH_Disabled="0"
  VSPCH_Create="1"
  VSPCH_Use="2"
  VSBasicRTChecks_Default="0"
  VSBasicRTChecks_StackFrames="1"
  VSBasicRTChecks_UninitVars="2"
  VSBasicRTChecks_Both="3"
  VSCompileAs_Default="0"
  VSCompileAs_C="1"
  VSCompileAs_Cpp="2"
  VSSubSystem_Console="1"
  VSSubSystem_Windows="2"
>
  <PropertyListOutputFormats>
    <PropertyList Name="LinkLibraries" Format="@(Value) " />
  </PropertyListOutputFormats>
</VS2008>

