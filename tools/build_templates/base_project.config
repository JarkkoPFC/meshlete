<Builds>
  <Build BuildName="debug"
         Build="debug"
         PreprocessorDefinitions="PFC_DEBUG;_DEBUG;@(PreprocessorDefinitions)"
  />
  <Build BuildName="release"
         Build="release"
         PreprocessorDefinitions="PFC_RELEASE;NDEBUG;@(PreprocessorDefinitions)"
  />
  <Build BuildName="retail"
         Build="retail"
         PreprocessorDefinitions="PFC_RETAIL;NDEBUG;@(PreprocessorDefinitions)"
  />
</Builds>

<Platforms>
  <Platform PlatformName="win32"
            VSPlatformType="Win32"
            CompilerName="@(DefaultCompilerName)"
            PreprocessorDefinitions="PFC_PLATFORM_WIN32;WIN32;_WINDOWS;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            LinkLibraries="user32.lib;gdi32.lib;ws2_32.lib;@(LinkLibraries)"
            ExeExt=".exe"
            LibExt=".lib"
            DllExt=".dll"
  />
  <Platform PlatformName="win64"
            VSPlatformType="x64"
            CompilerName="@(DefaultCompilerName)"
            PreprocessorDefinitions="PFC_PLATFORM_WIN64;WIN32;_WINDOWS;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            LinkLibraries="user32.lib;gdi32.lib;ws2_32.lib;@(LinkLibraries)"
            ExeExt=".exe"
            LibExt=".lib"
            DllExt=".dll"
  />
  <Platform PlatformName="linux32"
            CompilerName="gcc"
            PreprocessorDefinitions="PFC_PLATFORM_LINUX32;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            ExeExt=""
            LibExt=".a"
            DllExt=".so"
  />
  <Platform PlatformName="linux64"
            CompilerName="gcc"
            PreprocessorDefinitions="PFC_PLATFORM_LINUX64;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            ExeExt=""
            LibExt=".a"
            DllExt=".so"
  />
  <Platform PlatformName="x360"
            VSPlatformType="Xbox 360"
            CompilerName="@(DefaultCompilerName)"
            PreprocessorDefinitions="PFC_PLATFORM_X360;_XBOX;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            ExeExt=".exe"
            LibExt=".lib"
            DllExt=".dll"
  />
  <Platform PlatformName="durango"
            VSPlatformType="Durango"
            CompilerName="@(DefaultCompilerName)"
            PreprocessorDefinitions="PFC_PLATFORM_DURANGO;__WRL_NO_DEFAULT_LIB__;_XM_AVX_INTRINSICS_;_XM_F16C_INTRINSICS_;MONOLITHIC=1;@(PreprocessorDefinitions);@(DefaultCompilerDefinition)"
            LinkLibraries="kernelx.lib;combase.lib;ws2_32.lib;@(LinkLibraries)"
            VSCompileAsWinRT="true"
            ExeExt=".exe"
            LibExt=".lib"
            DllExt=".dll"
  />
  <Platform PlatformName="orbis"
            VSPlatformType="ORBIS"
            CompilerName="gcc"
            PreprocessorDefinitions="PFC_PLATFORM_ORBIS;@(PreprocessorDefinitions);PFC_COMPILER_GCC"
            LinkLibraries="-lScePosix_stub_weak;@(LinkLibraries)"
            ExeExt=".elf"
            LibExt=".a"
            DllExt=".so"
  />
</Platforms>
