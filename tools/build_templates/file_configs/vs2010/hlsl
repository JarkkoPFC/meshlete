      <Command Condition="'$(Configuration)|$(Platform)'=='@(BuildName)|@(VSPlatformType)'">"$(DXSDK_DIR)Utilities\bin\x86\fxc.exe" /nologo /E @(HLSLShaderMain) /T @(ShaderModel) @(HLSLAdditionalFlags) /Vn g_%(Filename) /Fh @(OutputDirectory)/%(Filename).sh %(FullPath)</Command>
      <Outputs Condition="'$(Configuration)|$(Platform)'=='@(BuildName)|@(VSPlatformType)'">@(OutputDirectory)/%(Filename).sh</Outputs>
