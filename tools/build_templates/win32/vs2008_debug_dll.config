		<Configuration
			Name="@(BuildName)|@(VSPlatformType)"
			OutputDirectory="@(OutputDirectory)"
			IntermediateDirectory="@(IntermediateDirectory)"
			ConfigurationType="2"
			CharacterSet="2"
			>
			<Tool
				Name="VCPreBuildEventTool"
			/>
			<Tool
				Name="VCCustomBuildTool"
			/>
			<Tool
				Name="VCXMLDataGeneratorTool"
			/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"
			/>
			<Tool
				Name="VCMIDLTool"
			/>
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="@(VSAdditionalOptions)"
				Optimization="@(VSDebugOptimization)"
				EnableIntrinsicFunctions="true"
				AdditionalIncludeDirectories="@(IncludeDirectories)"
				PreprocessorDefinitions="@(PreprocessorDefinitions)"
				StringPooling="true"
				MinimalRebuild="@(VSMinimalRebuild)"
				ExceptionHandling="1"
				BasicRuntimeChecks="@(VSDebugBasicRuntimeChecks)"
				RuntimeLibrary="3"
				FloatingPointModel="2"
				RuntimeTypeInfo="true"
				UsePrecompiledHeader="@(VSUsePrecompiledHeader)"
				PrecompiledHeaderThrough="@(PrecompiledHeader)"
				PrecompiledHeaderFile="@(IntermediateDirectory)/@(ProjectName)_@(BuildName).pch"
				ProgramDataBaseFileName="@(OutputDirectory)/@(ProjectName)_@(PlatformName)_@(BuildName).pdb"
				WarningLevel="@(VSWarningLevel)"
				DebugInformationFormat="3"
				CompileAs="@(VSCompileAs)"
				DisableSpecificWarnings="@(VSDisableSpecificWarnings)"
			/>
			<Tool
				Name="VCManagedResourceCompilerTool"
			/>
			<Tool
				Name="VCResourceCompilerTool"
			/>
			<Tool
				Name="VCPreLinkEventTool"
			/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="@(LinkLibraries)"
				OutputFile="@(OutputDirectory)/@(OutputFile)@(OutputFileExt)"
				ModuleDefinitionFile="@(VSModuleDefinitionFile)"
				LinkIncremental="2"
				AdditionalLibraryDirectories="@(LibraryDirectories)"
				GenerateDebugInformation="true"
				ProgramDataBaseFile="@(OutputDirectory)/@(ProjectName)_@(PlatformName)_@(BuildName).pdb"
				ImportLibrary="@(OutputDirectory)/@(OutputFile).lib"
			/>
			<Tool
				Name="VCALinkTool"
			/>
			<Tool
				Name="VCManifestTool"
				EmbedManifest="false"
			/>
			<Tool
				Name="VCXDCMakeTool"
			/>
			<Tool
				Name="VCBscMakeTool"
			/>
			<Tool
				Name="VCFxCopTool"
			/>
			<Tool
				Name="VCAppVerifierTool"
			/>
			<Tool
				Name="VCPostBuildEventTool"
			/>
		</Configuration>
